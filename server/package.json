{
  "name": "typescript-getting-started",
  "version": "1.0.0",
  "description": "A sample TypeScript app using Express",
  "main": "dist/index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "node .",
    "build": "tsc",
    "server": "nodemon",
    "client": "cd ../client/ && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "remove-dependecies:client": "rm -rf ../client/node_modules",
    "remove-dependecies:server": "rm -rf node_modules",
    "remove-dependecies:all": "npm run remove-dependecies:client && npm run remove-dependecies:server",
    "install-dependencies:client": "cd ../client && npm install",
    "install-dependencies:server": "npm install",
    "install-dependencies:all": "npm run install-dependencies:client && npm run install-dependencies:server",
    "prepare-project": "npm run remove-dependecies:all && npm run install-dependencies:all"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.7",
    "ts-node": "^10.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.7",
    "rxjs": "^7.3.0",
    "tsjs-xpx-chain-sdk": "^0.8.2",
    "winston": "^3.3.3",
    "winston-mongodb": "^5.0.7"
  },
  "nodemonConfig": {
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  }
}
